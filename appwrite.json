{
  "projectId": "68a36f6c002bfc1e6057",
  "projectName": "JagCodeDevOps",
  "functions": [
    {
      "name": "backend-api",
      "runtime": "node-18.0",
      "entrypoint": "src/index.ts",
      "path": "functions",
      "timeout": 30,
      "events": ["functions.*"],
      "variables": {
        "NODE_ENV": "production"
      }
    },
    {
      "name": "crypto-tracker",
      "runtime": "python-3.9",
      "entrypoint": "main.py",
      "path": "crypto-tracker",
      "timeout": 60,
      "events": ["databases.*"]
    },
    {
      "name": "defi-analyzer",
      "runtime": "python-3.9",
      "entrypoint": "main.py",
      "path": "defi-analyzer",
      "timeout": 60,
      "events": ["databases.*"]
    }
  ],
  "databases": [
    {
      "name": "jagcode_main",
      "collections": [
        {
          "name": "users",
          "attributes": [
            {
              "key": "email",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 255
            },
            {
              "key": "name",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 255
            },
            {
              "key": "profileImage",
              "type": "string",
              "status": "available",
              "required": false,
              "array": false,
              "size": 2048
            },
            {
              "key": "bio",
              "type": "string",
              "status": "available",
              "required": false,
              "array": false,
              "size": 1000
            },
            {
              "key": "portfolioValue",
              "type": "double",
              "status": "available",
              "required": false,
              "array": false
            },
            {
              "key": "isVerified",
              "type": "boolean",
              "status": "available",
              "required": false,
              "array": false
            }
          ],
          "indexes": [
            {
              "key": "email_index",
              "type": "unique",
              "attributes": ["email"]
            }
          ]
        },
        {
          "name": "crypto_data",
          "attributes": [
            {
              "key": "name",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 100
            },
            {
              "key": "symbol",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 20
            },
            {
              "key": "price",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 50
            },
            {
              "key": "change",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 50
            },
            {
              "key": "marketCap",
              "type": "string",
              "status": "available",
              "required": false,
              "array": false,
              "size": 100
            },
            {
              "key": "volume",
              "type": "string",
              "status": "available",
              "required": false,
              "array": false,
              "size": 100
            }
          ]
        },
        {
          "name": "nft_collection",
          "attributes": [
            {
              "key": "name",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 255
            },
            {
              "key": "artist",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 255
            },
            {
              "key": "price",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 50
            },
            {
              "key": "image",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 2048
            },
            {
              "key": "rarity",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 50
            },
            {
              "key": "description",
              "type": "string",
              "status": "available",
              "required": false,
              "array": false,
              "size": 1000
            }
          ]
        },
        {
          "name": "user_portfolios",
          "attributes": [
            {
              "key": "userId",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 50
            },
            {
              "key": "assetName",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 100
            },
            {
              "key": "symbol",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 20
            },
            {
              "key": "amount",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 50
            },
            {
              "key": "value",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 50
            },
            {
              "key": "change",
              "type": "string",
              "status": "available",
              "required": true,
              "array": false,
              "size": 50
            }
          ],
          "indexes": [
            {
              "key": "userId_index",
              "type": "key",
              "attributes": ["userId"]
            }
          ]
        }
      ]
    }
  ],
  "buckets": [
    {
      "name": "profile_images",
      "permissions": ["read(\"any\")", "write(\"users\")"],
      "fileSecurity": true,
      "enabled": true,
      "maximumFileSize": 5242880,
      "allowedFileExtensions": ["jpg", "jpeg", "png", "gif"],
      "compression": "gzip",
      "encryption": true,
      "antivirus": true
    },
    {
      "name": "nft_images",
      "permissions": ["read(\"any\")", "write(\"users\")"],
      "fileSecurity": true,
      "enabled": true,
      "maximumFileSize": 10485760,
      "allowedFileExtensions": ["jpg", "jpeg", "png", "gif", "webp"],
      "compression": "gzip",
      "encryption": true,
      "antivirus": true
    }
  ]
}