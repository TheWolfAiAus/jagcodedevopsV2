{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "outputDirectory": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_APPWRITE_ENDPOINT": "https://syd.cloud.appwrite.io/v1",
    "NEXT_PUBLIC_APPWRITE_PROJECT_ID": "68a36f6c002bfc1e6057",
    "NEXT_PUBLIC_APP_NAME": "JagCodeDevOps",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0"
  }
}